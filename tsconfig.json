{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noStrictGenericChecks": true,
    "pretty": true,
    "lib": [
      "dom",
      "esnext"
    ]
  }
}