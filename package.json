{
  "name": "trait",
  "version": "1.0.0",
  "description": "Trait is a better mixin",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:bios21/trait.git",
  "homepage": "https://github.com/bios21/trait",
  "bugs": {
    "url": "https://github.com/bios21/trait/issues"
  },
  "author": "Lilian Saget-Lethias <lilian.sagetlethias@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project tsconfig.base.json -c tslint.json '{src,test}/**/*.ts*'",
    "_webpack": "TS_NODE_PROJECT=\"config/webpack/tsconfig.json\" webpack --config config/webpack/webpack.config.ts",
    "start": "ts-node -P tsconfig.json ./example.ts",
    "build": "rimraf dist/* && yarn _webpack --env production -p --config-name build",
    "build:dev": "rimraf dist/* && yarn _webpack --env development --config-name build",
    "doc": "rimraf doc/* && typedoc src/",
    "pub:dev": "yarn build:dev && wsc pub:dev --no-build",
    "pub": "wsc pub"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^10.11.3",
    "@types/webpack": "^4.1.3",
    "@wynd/tslint-config-wynd": "^1.5.0",
    "@wynd/wynd-script": "^1.5.2",
    "git-message": "^2.0.2",
    "jest": "^24.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.1.1",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.1.3",
    "tslint": "^5.9.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^3.0.8"
  }
}
